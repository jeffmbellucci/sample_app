<% provide(:title, "Sign in") %>
<h1>Sign in</h1>

<%= render 'shared/error_messages'%>
	
	
	<%= form_for(:session, url: sessions_url) do |f| %>
	
	
	<%= f.label :email %>
	<%= f.text_field :email %>
	<br>
	<%= f.label :password %>
	<%=f.password_field :password %>
	<br>
	
	<%= f.submit "Submit the crap." %>
	<%end%>
	<br>
	<br>
	<p>New user? <%= link_to "Sign up now!", signup_url %></p>